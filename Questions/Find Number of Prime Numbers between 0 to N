#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin>>n;
    
    int cnt = 0;
    vector<int> isPrime(n+1, true);
    
    isPrime[0] = isPrime[1] = false;
    
    for(int i = 2 ; i <= n ; i++){
        
        if(isPrime[i]){
            cnt++;
            
            for(int j = i*2 ; j <= n ; j += i){
                isPrime[j] = false;
            }
        }
        
    }
    
    cout<<"No of prime numbers between 0 to "<<n<<" are : "<<cnt<<endl;

    return 0;
}
