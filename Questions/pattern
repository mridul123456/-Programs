// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

int main() {
    
    int cnt = 2;
    
    for(int i = 0 ; i < 7 ; i++){
        if(i % 3 == 0){
            cnt = cnt - (i/3);
        }
        else if(i % 5 == 0){
            cnt = cnt - 3;
        }
        for(int j = 0 ; j <= i ; j++){
            cout<<cnt<<" ";
            cnt++;
        }cout<<endl;
    }
    
}



2 
3 4 
5 6 7 
7 8 9 10 
11 12 13 14 15 
13 14 15 16 17 18 
17 18 19 20 21 22 23 
